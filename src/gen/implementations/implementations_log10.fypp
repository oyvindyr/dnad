#:def implementations_log10(dual_type, real_kind, number_of_derivatives) 
    elemental function log10_d(u) result(res)
        type(${dual_type}$), intent(in) :: u
        type(${dual_type}$) :: res

        real(${real_kind}$) :: inv

        inv = 1.0_${real_kind}$ / (u%x * log(10.0_${real_kind}$))
        res%x = log10(u%x)
        res%dx = u%dx * inv

    end function log10_d
#:enddef 
