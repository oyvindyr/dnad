  #:for dual_type, dual_sn in zip(dual_types, dual_shortnames)
    $:ge(dual_type, dual_sn, real_kind, is_pure, False, test_coverage)
  #:endfor
  #:for hdual_type, hdual_sn in zip(hdual_types, hdual_shortnames)
    $:ge(hdual_type, hdual_sn, real_kind, is_pure, True, test_coverage)
