#:if "sum" in interface_list 
  #:for dual_type, dual_sn in zip(dual_types, dual_shortnames)
    ${purity}$  function sum__${dual_sn}$(u) result(res)
        type(${dual_type}$), intent(in) :: u(:)
        type(${dual_type}$) :: res
        integer :: i

        res%f = sum(u%f)
        do i = 1, size(res%g)
            res%g(i) = sum(u%g(i))
        end do

    end function
  #:endfor
#:endif 
#:if "maxloc" in interface_list 
  #:for dual_type, dual_sn in zip(dual_types, dual_shortnames)
    ${purity}$  function maxloc__${dual_sn}$(array) result(ind)
        type(${dual_type}$), intent(in) :: array(:)
        integer :: ind(1)

        ind = maxloc(array%f)

    end function
  #:endfor
#:endif 
#:enddef 
